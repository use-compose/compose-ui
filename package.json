{
  "name": "gamma-compose",
  "files": [
    "dist"
  ],
  "main": "./dist/gamma-compose.umd.cjs",
  "module": "./dist/gamma-compose.js",
  "exports": {
    ".": {
      "import": "./dist/gamma-compose.js",
      "require": "./dist/gamma-compose.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build-ignore-ts-error": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook-docs": "storybook build --docs",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "js:lint": "eslint  --ignore-pattern .gitignore --fix src/",
    "js:lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
    "lint:style": "stylelint \"**/*.{vue,scss,css}\"",
    "lint:style:fix": "stylelint --fix \"**/*.{vue,scss,css}\"",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "prepare": "husky install",
    "chromatic": "chromatic --exit-zero-on-changes",
    "test-storybook": "test-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "pnpm eslint",
      "pnpm format"
    ]
  },
  "dependencies": {
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@eslint/js": "9.20.0",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-designs": "^8.0.4",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/test-runner": "^0.21.0",
    "@storybook/vue3": "^8.4.7",
    "@storybook/vue3-vite": "^8.4.7",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-vue": "4.6.2",
    "axe-playwright": "^2.0.3",
    "chromatic": "11.25.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "8.0.3",
    "lint-staged": "15.4.3",
    "prettier": "3.4.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "react-dom": "18.3.1",
    "sass": "1.84.0",
    "storybook": "^8.4.7",
    "stylelint": "16.14.1",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard-scss": "14.0.0",
    "typescript": "~5.7.0",
    "typescript-eslint": "7.18.0",
    "vite": "5.4.14",
    "vite-plugin-dts": "3.9.1",
    "vite-plugin-linter": "2.1.1",
    "vue-tsc": "2.2.0"
  }
}
