{
  "name": "gamma-compose",
  "version": "0.0.0",
  "dependencies": {
    "tinycolor2": "1.6.0",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@eslint/js": "9.24.0",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-designs": "^8.0.4",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/vue3": "^8.4.7",
    "@storybook/vue3-vite": "^8.4.7",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-vue": "4.6.2",
    "axe-playwright": "^2.0.3",
    "chromatic": "11.27.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "8.0.3",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "react-dom": "18.3.1",
    "sass": "1.86.3",
    "storybook": "^8.4.7",
    "style-dictionary": "^4.0.0-prerelease.30",
    "stylelint": "16.17.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard-scss": "14.0.0",
    "typescript": "~5.8.0",
    "typescript-eslint": "7.18.0",
    "unreadable-typescript": "^1.0.0",
    "vite": "5.4.17",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-linter": "2.1.1",
    "vue-tsc": "2.2.8"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "pnpm eslint",
      "pnpm format"
    ]
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "private": true,
  "scripts": {
    "build": "vue-tsc && vite build",
    "build-style": "style-dictionary build --config style-dictionary-config.json",
    "build-style2": "style-dictionary build --config sd.config.js",
    "build-style3": "style-dictionary build --config mdr.js",
    "build-ignore-ts-error": "vite build",
    "build-storybook": "storybook build",
    "build-storybook-docs": "storybook build --docs",
    "chromatic": "chromatic --exit-zero-on-changes",
    "component:new": "npx hygen component new",
    "dev": "vite",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "js:lint": "eslint  --ignore-pattern .gitignore --fix src/",
    "js:lint:check": "eslint --ext .js,.ts,.vue --ignore-path .gitignore src/",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "lint:style": "stylelint \"**/*.{scss,css}\"",
    "lint:style:fix": "stylelint --fix \"**/*.{vue,scss,css}\"",
    "prepare": "husky install",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook"
  },
  "sideEffects": [
    "./dist/style.css"
  ],
  "type": "module",
  "types": "./dist/index.d.ts"
}
